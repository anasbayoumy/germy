{
  "name": "ai-service",
  "version": "1.0.0",
  "description": "AI Service for Germy Attendance Platform - ArcFace, ML Analytics, Fraud Detection, and Gemini Chat",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:unit": "jest --testPathPattern=unit",
    "test:api": "jest --testPathPattern=api",
    "test:integration": "jest --testPathPattern=integration",
    "test:arcface": "jest --testPathPattern=arcface",
    "test:fraud": "jest --testPathPattern=fraud",
    "test:gemini": "jest --testPathPattern=gemini",
    "test:ml": "jest --testPathPattern=ml",
    "test:watch": "jest --watch",
    "test:verbose": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "DEBUG=* jest --runInBand",
    "test:all": "jest --coverage",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/db/seed.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts"
  },
  "dependencies": {
    "drizzle-orm": "^0.29.0",
    "postgres": "^3.4.3",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.22.4",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "axios": "^1.6.2",
    "sharp": "^0.33.0",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "@google/generative-ai": "^0.2.1",
    "aws-sdk": "^2.1508.0",
    "redis": "^4.6.12",
    "node-cron": "^3.0.3",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "prom-client": "^15.1.0",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "geolib": "^3.3.4",
    "exif-parser": "^0.1.12",
    "jimp": "^0.22.10",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "drizzle-kit": "^0.20.4",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@types/node": "^20.10.5",
    "@types/multer": "^1.4.11",
    "@types/axios": "^0.14.0",
    "@types/sharp": "^0.32.0",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "@types/lodash": "^4.14.202",
    "@types/node-cron": "^3.0.11",
    "@types/joi": "^17.2.3",
    "typescript": "^5.3.3",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.6.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "@types/supertest": "^2.0.16",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "prettier": "^3.1.1"
  },
  "keywords": [
    "ai",
    "attendance",
    "arcface",
    "fraud-detection",
    "gemini",
    "machine-learning",
    "facial-recognition",
    "saas",
    "microservice"
  ],
  "author": "Germy Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
